datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String      @id @default(uuid())
  email    String      @unique
  username String
  password String
}

model Venue {
  id       String      @id @default(uuid())
  name     String
  photo
  schedule String
  contact  String
}

model VenueTags {
  categories categoriesType
  cuisine    cuisineType
  features   featuresType
}

type categoriesType {
  type String[]
}

type cuisineType {
  type String[]
}

type featuresType {
  type String[]
}